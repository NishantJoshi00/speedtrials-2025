{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/map",
      "destination": "/map"
    },
    {
      "source": "/system/:path*",
      "destination": "/system/:path*"
    }
  ]
}